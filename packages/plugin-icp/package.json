{
    "name": "@elizaos/plugin-icp",
    "version": "0.1.5-alpha.1",
    "description": "ElizaOS plugin for Internet Computer Protocol integration with on-chain randomness and timers",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:e2e": "vitest run --config vitest.e2e.config.ts",
        "migrate": "prisma migrate dev",
        "migrate:deploy": "prisma migrate deploy",
        "generate": "prisma generate",
        "start": "node dist/index.js",
        "lint": "eslint src --ext .ts",
        "format": "prettier --write \"src/**/*.ts\""
    },
    "keywords": [
        "elizaos",
        "plugin",
        "icp",
        "internet-computer",
        "telegram",
        "meme-token",
        "randomness",
        "blockchain"
    ],
    "author": "asDNSk",
    "license": "MIT",
    "dependencies": {
        "@dfinity/agent": "^1.1.0",
        "@dfinity/candid": "^1.1.0",
        "@dfinity/principal": "^1.1.0",
        "@dfinity/identity": "^1.1.0",
        "@dfinity/auth-client": "^1.1.0",
        "@prisma/client": "^5.8.0",
        "prisma": "^5.8.0",
        "openai": "^4.24.1",
        "telegraf": "^4.15.0",
        "dotenv": "^16.3.1",
        "zod": "^3.22.4",
        "nanoid": "^5.0.4",
        "axios": "^1.6.5"
    },
    "devDependencies": {
        "@types/node": "^20.10.6",
        "typescript": "^5.3.3",
        "tsup": "^8.0.1",
        "vitest": "^1.1.1",
        "@typescript-eslint/eslint-plugin": "^6.17.0",
        "@typescript-eslint/parser": "^6.17.0",
        "eslint": "^8.56.0",
        "prettier": "^3.1.1"
    },
    "engines": {
        "node": ">=18.0.0",
        "pnpm": ">=8.0.0"
    }
}